<?xml version="1.0" encoding="US-ASCII"?>
<configuration>
  <buildSettings>
    <debug>true</debug>
  </buildSettings>
  <resolverSettings>
    <debug>true</debug>
    <localRepositories>
      <localRepository>.xm2</localRepository>
    </localRepositories>
    <jarRepositories>
      <jarRepository>/usr/share/java</jarRepository>
      <jarRepository>/usr/lib/java</jarRepository>
    </jarRepositories>
    <pomRepositories>
      <pomRepository>/usr/share/maven-poms</pomRepository>
      <pomRepository>/usr/share/maven/poms</pomRepository>
      <pomRepository>/usr/share/maven2/poms</pomRepository>
    </pomRepositories>
    <metadataRepositories>
      <metadataRepository>/usr/share/maven-fragments</metadataRepository>
    </metadataRepositories>
    <prefixes>
      <prefix>/</prefix>
    </prefixes>
    <blacklist>
      <blacklist>
        <groupId>javax.activation</groupId>
        <artifactId>activation</artifactId>
      </blacklist>
      <blacklist>
        <groupId>org.eclipse.jetty.orbit</groupId>
        <artifactId>javax.activation</artifactId>
      </blacklist>
      <blacklist>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav</artifactId>
      </blacklist>
      <blacklist>
        <groupId>org.apache.maven.wagon</groupId>
        <artifactId>wagon-webdav-jackrabbit</artifactId>
      </blacklist>
    </blacklist>
  </resolverSettings>
  <installerSettings>
    <jarDir>usr/share/java</jarDir>
    <jniDir>usr/lib/java</jniDir>
    <pomDir>usr/share/maven-poms</pomDir>
    <metadataDir>usr/share/maven-fragments</metadataDir>
  </installerSettings>
</configuration>
